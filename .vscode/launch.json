        {
            "name": "lazy_chores (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "lazy_chores (web)",
            "program": "lib/main.dart",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-port",
                "8080"
            ],
